{
  "name": "busynest",
  "description": "An Aurelia client application.",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pillaru/busynest.git"
  },
  "license": "MIT",
  "dependencies": {
    "aurelia-bootstrapper": "^2.3.3",
    "aurelia-configuration": "^1.0.17",
    "aurelia-event-aggregator": "^1.0.1",
    "aurelia-fetch-client": "^1.1.3",
    "aurelia-framework": "^1.1.5",
    "aurelia-google-analytics": "^2.3.1",
    "aurelia-history-browser": "^1.1.0",
    "aurelia-logging-console": "^1.0.0",
    "aurelia-pal": "^1.4.0",
    "aurelia-pal-browser": "^1.3.0",
    "aurelia-polyfills": "^1.2.2",
    "aurelia-route-recognizer": "^1.1.1",
    "aurelia-router": "^1.4.0",
    "aurelia-templating-binding": "^1.4.0",
    "aurelia-templating-resources": "^1.5.4",
    "aurelia-templating-router": "^1.2.0",
    "aurelia-validation": "^1.1.2",
    "bluebird": "^3.5.1",
    "bootstrap": "^4.0.0-beta.2",
    "font-awesome": "4.7.0",
    "gulp-rename": "^1.2.2",
    "isomorphic-fetch": "^3.0.0",
    "jquery": "^3.2.1",
    "moment": "^2.19.4",
    "numeral": "^2.0.6",
    "popper.js": "^1.12.6",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@aurelia/babel-jest": "^2.0.0-alpha.2",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/register": "^7.13.14",
    "aurelia-cli": "^2.0.2",
    "aurelia-loader-nodejs": "^1.1.0",
    "aurelia-loader-webpack": "^2.2.2",
    "aurelia-pal-nodejs": "^2.0.0",
    "aurelia-protractor-plugin": "^1.0.6",
    "aurelia-testing": "^1.0.0",
    "aurelia-webpack-plugin": "^4.0.0",
    "autoprefixer": "7.1.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-istanbul": "4.1.4",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-1": "^6.5.0",
    "clean-webpack-plugin": "^3.0.0",
    "codacy-coverage": "^2.0.3",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^0.28.7",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^7.23.0",
    "expose-loader": "^0.7.4",
    "file-loader": "^1.1.5",
    "gulp": "^4.0.2",
    "html-webpack-plugin": "^4.5.2",
    "htmlhint": "^0.14.2",
    "jest": "^26.6.3",
    "jest-transform-stub": "^2.0.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^1.4.0",
    "node-sass": "^4.7.2",
    "protractor": "^5.2.1",
    "pushstate-server": "^3.0.1",
    "sass-loader": "^6.0.6",
    "style-loader": "0.19.0",
    "wait-on": "2.0.2",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "lint:js": "eslint src test",
    "lint:html": "htmlhint -c .htmlhintrc src",
    "lint": "npm run lint:js && npm run lint:html",
    "pretest": "npm run lint",
    "build": "webpack --env.production --extractCss",
    "start": "webpack-dev-server --extractCss",
    "build:dev": "webpack --extractCss",
    "analyze": "webpack --env.production --analyze",
    "test": "au test"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^aurelia-binding$": "<rootDir>/node_modules/aurelia-binding"
    },
    "modulePaths": [
      "<rootDir>/src",
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.(css|less|sass|scss|styl|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "jest-transform-stub",
      "^.+\\.js$": "babel-jest"
    },
    "testRegex": "\\.spec\\.js$",
    "setupFiles": [
      "<rootDir>/test/jest-pretest.js"
    ],
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/*.spec.js",
      "!**/node_modules/**",
      "!**/test/**"
    ],
    "coverageDirectory": "<rootDir>/test/coverage-jest",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ]
  }
}
